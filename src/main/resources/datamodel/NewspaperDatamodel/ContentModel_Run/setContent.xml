<def:batchModify xmlns:def="http://www.fedora.info/definitions/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.fedora.info/definitions/ http://naiad:7880/schemaStore/batchModifySchema.xsd">


    <!--ContentModel_Run-->
    <def:modifyObject pid="doms:ContentModel_Run" state="I"
                      logMessage="Updating objects as part of a doms install/upgrade"/>
    <def:modifyDatastream pid="doms:ContentModel_Run" dsID="DS-COMPOSITE-MODEL"
                          dsLabel=""
                          dsMIME="text/xml"
                          formatURI="info:fedora/fedora-system:FedoraDSCompositeModel-1.0"
                          versionable="false"
                          altIDs="" dsControlGroupType="X" dsState="A"
                          checksumType="MD5"
                          logMessage="Updating objects as part of a doms install/upgrade">
        <def:xmlData>
            ${Run_DSCOMP_file_contents}
        </def:xmlData>
    </def:modifyDatastream>
    <def:modifyDatastream pid="doms:ContentModel_Run" dsID="RELS-EXT"
                          dsLabel="Relationships"
                          dsMIME="application/rdf+xml"
                          formatURI="info:fedora/fedora-system:FedoraRELSExt-1.0"
                          versionable="false"
                          altIDs="" dsControlGroupType="X" dsState="A"
                          checksumType="MD5"
                          logMessage="Updating objects as part of a doms install/upgrade">
        <def:xmlData>
            ${Run_RELS-EXT_file_contents}
        </def:xmlData>
    </def:modifyDatastream>
    <def:modifyDatastream pid="doms:ContentModel_Run" dsID="VIEW"
                          dsLabel="VIEW datastream"
                          dsMIME="application/rdf+xml"
                          versionable="false"
                          altIDs="" dsControlGroupType="X" dsState="A"
                          checksumType="MD5"
                          logMessage="Updating objects as part of a doms install/upgrade">
        <def:xmlData>
            ${Run_VIEW_file_contents}
        </def:xmlData>
    </def:modifyDatastream>
    <def:modifyDatastream pid="doms:ContentModel_Run" dsID="ONTOLOGY"
                          dsLabel="Local ontology"
                          dsMIME="application/rdf+xml"
                          formatURI="info:fedora/fedora-system:FedoraOntology-1.0"
                          versionable="false"
                          altIDs="" dsControlGroupType="X" dsState="A"
                          checksumType="MD5"
                          logMessage="Updating objects as part of a doms install/upgrade">
        <def:xmlData>
            ${Run_ONTOLOGY_file_contents}
        </def:xmlData>
    </def:modifyDatastream>

    <def:modifyDatastream pid="doms:ContentModel_Run" dsID="EVENTS_SCHEMA"
                          dsLabel="Events schema"
                          dsMIME="text/xml"
                          versionable="false"
                          altIDs="" dsControlGroupType="X" dsState="A"
                          checksumType="MD5"
                          logMessage="Updating objects as part of a doms install/upgrade">
        <def:xmlData>
            ${Run_EVENTS_SCHEMA_file}
        </def:xmlData>
    </def:modifyDatastream>

</def:batchModify>
