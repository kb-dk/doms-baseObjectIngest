<def:batchModify xmlns:def="http://www.fedora.info/definitions/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.fedora.info/definitions/ http://naiad:7880/schemaStore/batchModifySchema.xsd">

    <!--ContentModel_Run-->
    <def:addObject pid="doms:ContentModel_Run" label="Newspaper Run "
                   contentModel="string" logMessage="Updating objects as part of a doms install/upgrade"/>
    <def:modifyObject pid="doms:ContentModel_Run" state="I"
                      logMessage="Updating objects as part of a doms install/upgrade"/>
    <def:addDatastream pid="doms:ContentModel_Run" dsID="DS-COMPOSITE-MODEL"
                       dsLabel=""
                       dsMIME="text/xml"
                       formatURI="info:fedora/fedora-system:FedoraDSCompositeModel-1.0"
                       versionable="false"
                       altIDs="" dsControlGroupType="X" dsState="A"
                       checksumType="MD5"
                       logMessage="Updating objects as part of a doms install/upgrade">
        <def:xmlData>
            <placeholder/>
        </def:xmlData>
    </def:addDatastream>
    <def:addDatastream pid="doms:ContentModel_Run" dsID="RELS-EXT"
                       dsLabel="Relationships"
                       dsMIME="application/rdf+xml"
                       formatURI="info:fedora/fedora-system:FedoraRELSExt-1.0"
                       versionable="false"
                       altIDs="" dsControlGroupType="X" dsState="A"
                       checksumType="MD5"
                       logMessage="Updating objects as part of a doms install/upgrade">
        <def:xmlData>
            <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
                     xmlns:doms="http://doms.statsbiblioteket.dk/relations/default/0/1/#"
                     xmlns:ecm="http://ecm.sourceforge.net/relations/0/2/#">
                <rdf:Description rdf:about="info:fedora/doms:ContentModel_Run">
                    <hasModel rdf:resource="info:fedora/fedora-system:ContentModel-3.0"
                              xmlns="info:fedora/fedora-system:def/model#"/>
                    <hasModel rdf:resource="info:fedora/doms:ContentModel_DOMS"
                              xmlns="info:fedora/fedora-system:def/model#"/>
                    <ecm:extendsModel rdf:resource="info:fedora/doms:ContentModel_DOMS"/>
                    <doms:isPartOfCollection rdf:resource="info:fedora/doms:Newspaper_Collection"/>
                    <doms:hasLicense rdf:resource="info:fedora/doms:Open_License"/>
                </rdf:Description>
            </rdf:RDF>
        </def:xmlData>

    </def:addDatastream>
    <def:addDatastream pid="doms:ContentModel_Run" dsID="VIEW"
                       dsLabel="VIEW datastream"
                       dsMIME="application/rdf+xml"
                       versionable="false"
                       altIDs="" dsControlGroupType="X" dsState="A"
                       checksumType="MD5"
                       logMessage="Updating objects as part of a doms install/upgrade">
        <def:xmlData>
            <placeholder/>
        </def:xmlData>

    </def:addDatastream>
    <def:addDatastream pid="doms:ContentModel_Run" dsID="ONTOLOGY"
                       dsLabel="Local ontology"
                       dsMIME="application/rdf+xml"
                       formatURI="info:fedora/fedora-system:FedoraOntology-1.0"
                       versionable="false"
                       altIDs="" dsControlGroupType="X" dsState="A"
                       checksumType="MD5"
                       logMessage="Updating objects as part of a doms install/upgrade">
        <def:xmlData>
            <placeholder/>
        </def:xmlData>
    </def:addDatastream>

    <def:addDatastream pid="doms:ContentModel_Run" dsID="EVENTS_SCHEMA"
                       dsLabel="Events schema"
                       dsMIME="text/xml"
                       versionable="false"
                       altIDs="" dsControlGroupType="X" dsState="A"
                       checksumType="MD5"
                       logMessage="Updating objects as part of a doms install/upgrade">
        <def:xmlData>
            <placeholder/>
        </def:xmlData>

    </def:addDatastream>


</def:batchModify>
