<ns:methods xmlns:ns="http://ecm.sourceforge.net/types/methods/0/2/#">
  <ns:method>
    <ns:name>VHS import</ns:name>
    <ns:command>$METHOD_SCRIPTS/invoke.sh -inputvalue domsUser %%domsUser%% -inputvalue domsPass %%domsPassword%% %%quality%% %%file%% %%channelID%% %%timeStart%% %%timeEnd%% %%vhs-label%% %%recorder%%</ns:command>
    <ns:type>static</ns:type>
    <ns:parameters>
      <ns:parameter>
        <ns:name>quality</ns:name>
        <ns:parameterprefix>-inputvalue quality </ns:parameterprefix>
        <ns:required>true</ns:required>
        <ns:repeatable>false</ns:repeatable>
        <ns:type>Integer</ns:type>
      </ns:parameter>
      <ns:parameter>
        <ns:name>file</ns:name>
        <ns:parameterprefix>-inputvalue file </ns:parameterprefix>
        <ns:required>true</ns:required>
        <ns:repeatable>false</ns:repeatable>
        <ns:type>ServerFile</ns:type>
        <ns:config>/fedora/vhssamplefiles</ns:config>
      </ns:parameter>
      <ns:parameter>
        <ns:name>channelID</ns:name>
        <ns:parameterprefix>-inputvalue channelid </ns:parameterprefix>
        <ns:required>true</ns:required>
        <ns:repeatable>false</ns:repeatable>
        <ns:type>Text</ns:type>
      </ns:parameter>
      <ns:parameter>
        <ns:name>timeStart</ns:name>
        <ns:parameterprefix>-inputvalue starttime </ns:parameterprefix>
        <ns:required>true</ns:required>
        <ns:repeatable>false</ns:repeatable>
        <ns:type>Datetime</ns:type>
      </ns:parameter>
      <ns:parameter>
        <ns:name>timeEnd</ns:name>
        <ns:parameterprefix>-inputvalue stoptime </ns:parameterprefix>
        <ns:required>true</ns:required>
        <ns:repeatable>false</ns:repeatable>
        <ns:type>Datetime</ns:type>
      </ns:parameter>
      <ns:parameter>
        <ns:name>vhs-label</ns:name>
        <ns:parameterprefix>-inputvalue vhslabel </ns:parameterprefix>
        <ns:required>true</ns:required>
        <ns:repeatable>false</ns:repeatable>
        <ns:type>TextBox</ns:type>
      </ns:parameter>
      <ns:parameter>
        <ns:name>recorder</ns:name>
        <ns:parameterprefix>-inputvalue recorder </ns:parameterprefix>
        <ns:required>true</ns:required>
        <ns:repeatable>false</ns:repeatable>
        <ns:type>Text</ns:type>
      </ns:parameter>
    </ns:parameters>
  </ns:method>
</ns:methods>