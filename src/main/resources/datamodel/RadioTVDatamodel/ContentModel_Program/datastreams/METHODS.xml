<ns:methods xmlns:ns="http://ecm.sourceforge.net/types/methods/0/2/#">
    <ns:method>
        <ns:name>VHS clip import</ns:name>
        <ns:command>$METHOD_SCRIPTS/vhsclip.sh -inputvalue domsPid %%domsPid%% -inputvalue domsUser %%domsUser%% -inputvalue domsPassword %%domsPassword%% %%File%%</ns:command>
        <ns:type>dynamic</ns:type>
        <ns:parameters>
            <ns:parameter>
                <ns:name>File</ns:name>
                <ns:parameterprefix>-inputvalue mpgfile </ns:parameterprefix>
                <ns:required>true</ns:required>
                <ns:repeatable>false</ns:repeatable>
                <ns:type>ServerFile</ns:type>
                <ns:config>/home/doms/vhsclips</ns:config>
            </ns:parameter>
        </ns:parameters>
    </ns:method>
</ns:methods>
